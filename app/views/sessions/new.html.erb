<div class="col-sm-6 col-sm-offset-1">
<%= form_for :session, url:{action: "create"} do |f| %>
<fieldset>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div class="form-group">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <%= f.submit 'Log Me In' %>
</fieldset>
<% end %>
</div>